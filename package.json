{
    "name": "wheel-of-names-helper",
    "version": "2.3.1",
    "main": "dist-electron/main/main.js",
    "description": "Wheel of names tool for Twitch streamers",
    "author": "SnekCode",
    "repository": "https://github.com/SnekCode/WheelOfNamesHelper",
    "license": "MIT",
    "private": false,
    "keywords": [
        "electron",
        "rollup",
        "vite",
        "vue3",
        "vue"
    ],
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
        }
    },
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build && electron-builder --publish never",
        "preview": "vite preview",
        "publish": "vue-tsc --noEmit && vite build && electron-builder --publish always",
        "dumbtest": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" npx jest -c=jest.config.cjs --no-cache",
        "test": "jest -c=jest.config.cjs --no-cache",
        "version:major": "npm version major",
        "version:minor": "npm version minor",
        "version:patch": "npm version patch",
        "version:current": "npm version from-git --no-git-tag-version",
        "version:major:beta": "npm version premajor --preid=beta",
        "version:minor:beta": "npm version preminor --preid=beta",
        "version:patch:beta": "npm version prepatch --preid=beta",
        "version:current:beta": "npm version from-git --preid=beta --no-git-tag-version",
        "version:pre:beta": "npm version prerelease --preid=beta"
    },
    "devDependencies": {
        "@types/axios": "^0.14.4",
        "@types/crypto-js": "^4.2.2",
        "@types/electron-prompt": "^1.6.5",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.8.6",
        "@types/tmi.js": "^1.8.6",
        "@vitejs/plugin-vue": "^5.0.4",
        "cross-env": "^7.0.3",
        "electron": "^33.0.2",
        "electron-builder": "^25.1.8",
        "electron-vite": "^2.3.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.6",
        "typescript": "^5.5.4",
        "vite": "^5.4.11",
        "vite-plugin-electron": "^0.28.4",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vue": "^3.4.21",
        "vue-tsc": "^2.1.10"
    },
    "dependencies": {
        "@twurple/auth": "^7.2.0",
        "@twurple/auth-ext": "^7.2.0",
        "axios": "^1.7.7",
        "crypto-js": "^4.2.0",
        "electron-forge": "^5.2.4",
        "electron-log": "^5.2.0",
        "electron-prompt": "^1.7.0",
        "electron-store": "^10.0.0",
        "electron-updater": "^6.3.9",
        "googleapis": "^144.0.0",
        "http": "^0.0.1-security",
        "keytar": "^7.9.0",
        "marked": "^15.0.0",
        "open": "^10.1.0",
        "tmi.js": "^1.8.5",
        "twitch-auth": "^4.6.7",
        "twitch-chat-client": "^4.6.7",
        "url": "^0.11.4",
        "vite-tsconfig-paths": "^5.0.1",
        "vue-router": "^4.5.0",
        "youtube-chat": "^2.2.0"
    }
}
